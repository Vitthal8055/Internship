   "source": [
    "fact = 1\n",
    "num = int(input(\"Enter the number : \"))\n",
    "while num > 0:\n",
    "    fact = fact * num\n",
    "    num = num - 1\n",
    "print (\"The factorial of \", num , \"is\" , fact)   \n"
